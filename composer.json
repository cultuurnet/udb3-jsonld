{
  "name": "cultuurnet/udb3-models",
  "description": "",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [],
  "require": {
    "php": "~5.6 || >=7.0",
    "cultuurnet/geocoding": "~0.1",
    "moneyphp/money": "~1.3",
    "respect/validation": "~1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "squizlabs/php_codesniffer": "~2.3",
    "phing/phing": "~2.11",
    "satooshi/php-coveralls": "~0.7",
    "symfony/serializer": "~2.3"
  },
  "suggest": {
    "symfony/serializer": "To deserialize JSON-LD document to UDB3 models"
  },
  "autoload": {
    "psr-4": {
      "CultuurNet\\UDB3\\Model\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CultuurNet\\UDB3\\Model\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "scripts": {
    "phing": "./vendor/bin/phing test"
  }
